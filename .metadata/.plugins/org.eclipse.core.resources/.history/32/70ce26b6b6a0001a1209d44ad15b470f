package exam_01;

import java.util.Scanner;

public class StudentsScore {
	public static void main(String[] args) {
		int score[] = new int[10];
		int sub[] = new int[10];
		int k[] = new int [10];
		int e[] = new int [10];
		int m[] = new int [10];
		
		Scanner scan = new Scanner(System.in);
		
		for(int i = 0; i <= 9; i++) {
			System.out.println("학생의 점수를 입력하시오");
			k[i] = scan.nextInt();
			System.out.println("학생의 점수를 입력하시오");
			e[i] = scan.nextInt();
			System.out.println("학생의 점수를 입력하시오");
			m[i] = scan.nextInt();
			
		}
		
		
		System.out.println("학생의 점수를 입력하시오");
		
	}

}
